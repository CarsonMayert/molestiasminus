{
  "name": "molestiasminus",
  "description": "A simple, maximally extensible, dependency minimized framework for building modern Ethereum dApps",
  "keywords": [
    "react",
    "at",
    "itaque",
    "ut",
    "javascript",
    "enim",
    "web3",
    "omnis",
    "frontend",
    "dapp",
    "metamask",
    "walletconnect",
    "walletlink",
    "frame",
    "aut",
    "omnis",
    "squarelink",
    "itaque",
    "sunt",
    "magic"
  ],
  "author": "AlanisHomenick",
  "repository": {
    "type": "git",
    "url": "https://github.com/CarsonMayert/molestiasminus.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "publish:lerna": "lerna publish",
    "clean": "lerna clean --yes && lerna exec -- rimraf yarn.lock dist/ .rts2_cache_{cjs,esm}/",
    "build": "lerna run build",
    "start": "lerna run --parallel --no-bail start -- --noClean",
    "lint": "lerna run lint --parallel",
    "new-version": "lerna version --conventional-commits --yes",
    "publish": "lerna publish from-git",
    "test": "lerna run test --parallel"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "lerna": "^3.19.0",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "rimraf": "^3.0.1",
    "tsdx": "^0.11.0"
  },
  "license": "GPL-3.0-or-later",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "homepage": "https://github.com/CarsonMayert/molestiasminus.git",
  "version": "1.0.0"
}